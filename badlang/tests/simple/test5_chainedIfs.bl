/*
Test control flow with if statements.
*/

fun int main() {
    int x = 5;
    int y = 3;
    int z = 10;

    if (x > 0) {
        println 1;

        if (y < 5) {
            println 2;
        } else if (y == 5) {
            println 3;
        } else {
            println 4;
        }

        if (z >= 10) {
            println 5;

            if (x + y > z) {
                println 6;
            } else {
                println 7;
            }
        }
    } else if (x == 0) {
        println 8;
    } else {
        println 9;
    }

    if (x < 10) {
        println 10;

        if (y > 0) {
            println 11;
        }

        if (z != 0) {
            println 12;
        }
    }

    return 0;
}
